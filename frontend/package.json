{
  "name": "drupal-gatsby-responsive-images",
  "private": true,
  "description": "Responsive image experiment for Drupal Gatsby to avoid image processing on build.",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "cross-env GATSBY_GRAPHQL_IDE=playground gatsby develop --host 0.0.0.0",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve --host 0.0.0.0",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "analyze": "ANALYZE_BUNDLE_SIZE=true gatsby build"
  },
  "dependencies": {
    "@apollo/react-common": "^3.1.4",
    "@apollo/react-hooks": "^4.0.0",
    "@babel/core": "^7.10.5",
    "@mdx-js/mdx": "^1.6.16",
    "@mdx-js/react": "^1.6.16",
    "gatsby": "^2.18.6",
    "gatsby-background-image": "^1.1.1",
    "gatsby-image": "^2.4.16",
    "gatsby-plugin-apollo": "^3.0.1",
    "gatsby-plugin-emotion": "^4.3.10",
    "gatsby-plugin-mdx": "^1.2.34",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sass": "^2.3.12",
    "gatsby-plugin-sharp": "^2.6.27",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-remark-images": "^3.3.25",
    "gatsby-source-filesystem": "^2.3.24",
    "gatsby-source-graphql": "^2.7.1",
    "gatsby-transformer-sharp": "^2.5.13",
    "graphql-tag": "^2.11.0",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "typescript": "^2.9.2"
  },
  "devDependencies": {
    "cross-env": "^6.0.3",
    "prettier": "^1.19.1"
  },
  "repository": {
    "type": "git",
    "url": ""
  }
}
